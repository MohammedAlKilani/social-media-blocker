{
    "manifest_version": 2,
    "name": "Ad-scan",
    "version": "0.1",
    "permissions":["webRequest","storage"],
    "content_scripts": [
    {
    "js": ["inject.js"],
    "matches": ["<all_urls>"],
    "run_at": "document_start",
    "all_frames": true
    }]
   ,"browser_action":{
    "default_popup":"popup.html"
   }
}