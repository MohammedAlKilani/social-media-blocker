{
    "manifest_version": 2,
    "name": "socialMediaBlocker",
    "version": "0.1",
    "options_page":"options.html",
    "permissions":["webRequest","storage"],
    "content_scripts": [
    {
    "js": ["inject.js"],
    "matches": ["<all_urls>"],
    "run_at": "document_start",
    "all_frames": true
    }]
   ,"browser_action":{
    "default_popup":"popup.html"
   }
   ,
   "icons": {
    "16": "ad-blocker.png",
    "32": "ad-blocker.png",
    "48": "ad-blocker.png",
    "128": "ad-blocker.png"
  }
   
}